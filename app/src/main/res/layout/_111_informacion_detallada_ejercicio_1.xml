    <TextView
        android:id="@+id/tituloEquipo_111"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Equipo"
        android:textColor="@color/white"
        android:textSize="30sp" />

    <androidx.recyclerview.widget.RecyclerView
