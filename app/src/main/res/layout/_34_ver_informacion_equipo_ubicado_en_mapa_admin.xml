<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:padding="16dp"
    android:background="@android:color/transparent">

    <ImageView
        android:id="@+id/rect_fondo_gris"
        android:layout_width="381dp"
        android:layout_height="664dp"
        android:layout_marginBottom="16dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rect_redondeado_gris_medio" />

    <ImageButton
        android:id="@+id/btnX_34"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginTop="12dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.955"
        app:layout_constraintStart_toStartOf="@+id/rect_fondo_gris"
        app:layout_constraintTop_toTopOf="@+id/rect_fondo_gris"
        app:srcCompat="@drawable/icon_x_ne" />

    <TextView
        android:id="@+id/nombreEquipo_34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="setTextNombreEquipo"
        android:textColor="@color/black"
        android:textSize="22sp"
        app:layout_constraintEnd_toStartOf="@+id/btnX_34"
        app:layout_constraintHorizontal_bias="0.728"
        app:layout_constraintStart_toStartOf="@+id/rect_fondo_gris"
        app:layout_constraintTop_toTopOf="@+id/rect_fondo_gris" />

    <ImageView
        android:id="@+id/setImageEquipo_34"
        android:layout_width="342dp"
        android:layout_height="210dp"
        android:layout_marginTop="24dp"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="@+id/rect_fondo_gris"
        app:layout_constraintHorizontal_bias="0.477"
        app:layout_constraintStart_toStartOf="@+id/rect_fondo_gris"
        app:layout_constraintTop_toBottomOf="@+id/nombreEquipo_34"
        app:srcCompat="@drawable/cuadrado_gr" />

    <TextView
        android:id="@+id/textMusculos_34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Músculos"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/rect_fondo_gris"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/rect_fondo_gris"
        app:layout_constraintTop_toBottomOf="@+id/setImageEquipo_34" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerViewItemMusculoInvolucrado_34"
        android:layout_width="345dp"
        android:layout_height="73dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="@+id/rect_fondo_gris"
        app:layout_constraintHorizontal_bias="0.536"
        app:layout_constraintStart_toStartOf="@+id/rect_fondo_gris"
        app:layout_constraintTop_toBottomOf="@+id/textMusculos_34" />

    <TextView
        android:id="@+id/textReferencia_34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Referencia"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/rect_fondo_gris"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/rect_fondo_gris"
        app:layout_constraintTop_toBottomOf="@+id/recyclerViewItemMusculoInvolucrado_34" />

    <TextView
        android:id="@+id/setTextReferencia_34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="setTextReferencia"
        android:textSize="13sp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/rect_fondo_gris"
        app:layout_constraintStart_toStartOf="@+id/rect_fondo_gris"
        app:layout_constraintTop_toBottomOf="@+id/textReferencia_34" />

    <TextView
        android:id="@+id/textTipoMaquina_34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Tipo máquina"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/rect_fondo_gris"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/rect_fondo_gris"
        app:layout_constraintTop_toBottomOf="@+id/setTextReferencia_34" />


    <TextView xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:textSize="14sp"
        android:gravity="center_horizontal"
        android:paddingLeft="13dp"
        android:paddingRight="13dp"
        android:layout_marginBottom="9dp"
        android:background="@drawable/rounded_orange_background"
        android:text="Tipo máquina"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/rect_fondo_gris"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/rect_fondo_gris"
        app:layout_constraintTop_toBottomOf="@+id/textTipoMaquina_34"
        android:id="@+id/itemTipoMaquina_34"/>

    <TextView
        android:id="@+id/textDescripcion_34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Descripción"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/rect_fondo_gris"
        app:layout_constraintStart_toStartOf="@+id/rect_fondo_gris"
        app:layout_constraintTop_toBottomOf="@+id/itemTipoMaquina_34" />

    <TextView
        android:id="@+id/setTextDescripcion_34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set Text Descripcion"
        app:layout_constraintEnd_toEndOf="@+id/rect_fondo_gris"
        app:layout_constraintStart_toStartOf="@+id/rect_fondo_gris"
        app:layout_constraintTop_toBottomOf="@+id/textDescripcion_34" />

    <Button
        android:id="@+id/btnCambiarEquipo_34"
        android:layout_width="357dp"
        android:layout_height="59dp"
        android:layout_marginTop="32dp"
        android:backgroundTint="#FDA400"
        android:text="Cambiar equipo"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="@+id/rect_fondo_gris"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/setTextDescripcion_34"
        app:layout_constraintVertical_bias="0.136" />


</androidx.constraintlayout.widget.ConstraintLayout>
