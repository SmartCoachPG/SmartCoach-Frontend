<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/f3">

    <TextView
        android:id="@+id/tituloSuscríbete_126"
        android:layout_width="362dp"
        android:layout_height="83dp"
        android:layout_marginTop="28dp"
        android:text="Suscríbete en un gimnasio para acceder a más información"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/rectBusqueda_126"
        android:layout_width="353dp"
        android:layout_height="43dp"
        android:layout_marginTop="52dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tituloSuscríbete_126"
        app:srcCompat="@drawable/rect_redondeado_gris_medio" />

    <EditText
        android:id="@+id/editTextGym_126"
        android:layout_width="273dp"
        android:layout_height="36dp"
        android:background="@null"
        android:ems="10"
        android:hint="Stark"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="@+id/rectBusqueda_126"
        app:layout_constraintEnd_toEndOf="@+id/rectBusqueda_126"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="@+id/rectBusqueda_126"
        app:layout_constraintTop_toTopOf="@+id/rectBusqueda_126"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/btnBusquedaGym_126"
        android:layout_width="38dp"
        android:layout_height="38dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/rectBusqueda_126"
        app:layout_constraintEnd_toEndOf="@+id/rectBusqueda_126"
        app:layout_constraintHorizontal_bias="0.782"
        app:layout_constraintStart_toEndOf="@+id/editTextGym_126"
        app:layout_constraintTop_toTopOf="@+id/rectBusqueda_126"
        app:layout_constraintVertical_bias="0.222"
        app:srcCompat="@drawable/icon_lupa_rellena_na" />

    <include
        layout="@layout/_otros_menu_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/descripcion_126"
        android:layout_width="330dp"
        android:layout_height="125dp"
        android:layout_marginTop="192dp"
        android:text="No se ha registrado ningún gimnasio aún, pero pronto podrás verlos en la aplicación"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectBusqueda_126" />


</androidx.constraintlayout.widget.ConstraintLayout>
